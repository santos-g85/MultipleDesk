<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@(ViewData["Title"] ?? "MultipleDesk") - Secure Remote Access</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="icon" type="image/x-icon" href="~/tabicon.ico" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MultipleDesk.styles.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

    <!--bot-->
    <meta name="keywords" content="Remote Access, Remote Desktop, Secure Connection, MultipleDesk, Remote Support" />
    <meta name="robots" content="index, follow" />

    <meta name="title" content="MultipleDesk - Secure Remote Access" />
    <meta name="description" content="Connect and control any device instantly with MultipleDesk. Secure, fast, and simple." />


    <!-- meta-->
    <meta property="og:title" content="MultipleDesk - Secure Remote Access" />
    <meta property="og:description" content="Connect and control any device instantly with MultipleDesk. Secure, fast, and simple." />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="http://multipledesk.com/" />
    <meta property="og:image" content="http://multipledesk.com/images/logohere.png" />
    <meta property="og:image:alt" content="MultipleDesk Remote Access Devices" />

    <!-- twitter meta-->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content="MultipleDesk - Secure Remote Access" />
    <meta name="twitter:description" content="Connect and control any device instantly with MultipleDesk. Secure, fast, and simple." />
    <meta name="twitter:image" content="http://multipledesk.com/images/logohere.png" />

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="footer text-muted">
        <div class="container">
            &copy; 2025 -  <a href="https://www.jooneli.com/">Jooneli Inc</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>


